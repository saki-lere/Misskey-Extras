### {
  name: "Denyanify"
  version: "1.0.0"
  author:  "volpeon"
  description: "Keep your posts free of auto-inserted nyans"
  permission: []
}

@denyanifyText(note) {
  if ((note.text == null) || (note.text == "")) {
    return note
  }

  note.text = note.text.replace("nya", "na")
  note
}

Plugin:register_note_post_interruptor(denyanifyText)
